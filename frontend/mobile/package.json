{
  "name": "mapyourworld-mobile",
  "version": "1.0.0",
  "description": "Aplicación móvil y web de MapYourWorld con React Native y React",
  "main": "App.tsx",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build-web": "webpack --mode production",
    "eject": "expo eject",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "clean": "npx rimraf node_modules package-lock.json && npm install --force"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
